    <param name="filename">dbus-factory</param>
    <param name="versionformat">git%cd~%h</param>
    <param name="versionprefix">0.0.2</param>
  </service>
  <service name="recompress">
    <param name="file">*.tar</param>
    <param name="compression">xz</param>
  </service>
