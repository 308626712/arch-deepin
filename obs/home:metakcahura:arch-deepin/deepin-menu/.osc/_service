    <param name="scm">git</param>
    <param name="url">git://github.com/linuxdeepin/deepin-menu.git</param>
    <param name="filename">deepin-menu</param>
    <param name="versionformat">git%cd~%h</param>
    <param name="versionprefix">1.1</param>
    <param name="revision">xmenu</param>
  </service>
  <service name="recompress">
    <param name="file">*.tar</param>
    <param name="compression">xz</param>
  </service>
  <service name="set_version"/>
</services>